{
	"parser": "babel-eslint",
	"settings": {
		"ecmascript": 6,
		"jsx": true
	},
	"env": {
		"browser": true
	},
	"plugins": [
		"react"
	],
	"extends": "airbnb",
	"rules": {
		"strict": 2,
		"semi": 2,
		"no-unused-vars": 2,
		"camelcase": 2,
		"indent": [2, "tab"],
		"import/no-unresolved": [2, {
			"ignore": [
				"^expose?.*!.*$",
				"react",
				"react-addons-css-transition-group"
			]
		}],
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"],
		"react/prefer-stateless-function": 0
	}
}
